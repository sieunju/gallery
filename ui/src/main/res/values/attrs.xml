<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GalleryRecyclerView">
        <attr name="galleryCameraVisible" format="boolean" />
        <attr name="galleryCameraDrawableId" format="integer" />
        <attr name="gallerySelectedMaxCount" format="integer" />
        <attr name="gallerySelectedSize" format="dimension" />
        <attr name="gallerySelectedCorner" format="dimension" />
        <attr name="gallerySelectedStroke" format="dimension" />
        <attr name="gallerySelectedStrokeColor" format="color" />
        <attr name="gallerySelectedBgColor" format="color" />
        <attr name="gallerySelectedTxtColor" format="color" />
        <attr name="gallerySelectedGravity">
            <flag name="top" value="0x30" />
            <flag name="bottom" value="0x50" />
            <flag name="left" value="0x03" />
            <flag name="right" value="0x05" />
        </attr>
        <attr name="gallerySelectedBgDim" format="color" />
    </declare-styleable>

</resources>